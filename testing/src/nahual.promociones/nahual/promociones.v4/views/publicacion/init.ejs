<h2>Publicaciones</h2>
<a href='javascript://' class='add'>Agregar Nueva Publicación</a>
<table class="list" cellspacing='0px'>
	<thead>
	<tr>
		<% for(var i = 0; i < Nahual.Promociones.Models.Publicacion.editableAttributes.length; ++i){ 
			var attr = Nahual.Promociones.Models.Publicacion.editableAttributes[i];
			if (attr.ShowInList) {
		%>
				<th><%= attr.Description%> </th>	 
		<%
			}
		}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= controller.view('list',{publicaciones: publicaciones})%>
	</tbody>
</table>
